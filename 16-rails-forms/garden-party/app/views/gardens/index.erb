<h1>Welcome to the Garden Party</h1>

<% @gardens.each do |garden| %>
  <h3>
    <%# link_to garden.name, "/gardens/#{garden.id}" %>
    <%# link_to garden.name, garden_path(garden.id) %>
    <%# link_to garden.name, garden_path(garden) %>
    <%= link_to garden.name, garden %>
  </h3>
  <em><%= garden.location %></em>
  <hr>
<% end %>